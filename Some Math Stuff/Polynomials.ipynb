   "source": [
    "# Pascal's Triangle"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "pascal = [[1]]\n",
    "\n",
    "for i in range(10):\n",
    "    \n",
    "    last = pascal[-1]\n",
    "    this = [1]\n",
    "    for x, y in zip(last[:-1], last[1:]):\n",
    "        this.append(x+y)\n",
    "    this.append(1)\n",
    "    pascal.append(this)"
   ]
